<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Check in time</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.check_in_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
